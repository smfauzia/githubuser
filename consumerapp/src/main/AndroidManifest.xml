<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.latihan.consumerapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="com.latihan.githubuser.READ_DATABASE" />
    <uses-permission android:name="com.latihan.githubuser.WRITE_DATABASE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/githublogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/githublogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.GithubUser">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".alarmreminder.ReminderActivity" />
        <activity android:name=".DetailActivity" />
        <activity android:name=".Favorite.FavoriteActivity" />
    </application>

    <queries>
        <package android:name="com.latihan.githubuser" />
    </queries>

</manifest>